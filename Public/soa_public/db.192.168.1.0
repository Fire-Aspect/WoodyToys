$TTL 86400
@ IN SOA soa-public.m1-1.ephec-ti.be. admin.m1-1.ephec-ti.be. (
    2023032102 ; serial number
    3600 ; refresh
    1800 ; retry
    604800 ; expire
    86400 ; default TTL
)

; Define name servers
@ IN NS soa-public.m1-1.ephec-ti.be.

; Define PTR records for IP addresses in the 192.168.1.0/24 subnet
2	IN	PTR	www.m1-1.ephec-ti.be.
3	IN	PTR	resolver.m1-1.ephec-ti.be.
5	IN	PTR	mail.m1-1.ephec-ti.be.
4	IN	PTR	soa-public.m1-1.ephec-ti.be.
